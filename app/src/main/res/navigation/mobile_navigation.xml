<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_loginActivity">


    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.projectdva232v1.ui.HomePage.homePageFragment"
        android:label="Home Page"
        tools:layout="@layout/fragment_home_page" >
        <action
            android:id="@+id/action_nav_home_to_nav_reading"
            app:destination="@id/nav_reading" />
        <action
            android:id="@+id/action_nav_home_to_nav_listening"
            app:destination="@id/nav_listening" />
        <action
            android:id="@+id/action_nav_home_to_nav_vocabulary"
            app:destination="@id/nav_vocabulary" />
        <action
            android:id="@+id/action_nav_home_to_nav_writing"
            app:destination="@id/nav_writing" />
        <action
            android:id="@+id/action_nav_home_to_nav_speaking"
            app:destination="@id/nav_speaking" />
    </fragment>
    <fragment
        android:id="@+id/nav_listening"
        android:name="com.example.projectdva232v1.ui.Listening.ListeningFragment"
        android:label="@string/listening"
        tools:layout="@layout/fragment_reading_activity">
        <action
            android:id="@+id/action_nav_listening_to_nav_example5"
            app:destination="@id/nav_example5" />
        <argument
            android:name="ListeningFragmentArgs"
            app:argType="string"
            android:defaultValue="All" />
    </fragment>
    <fragment
        android:id="@+id/nav_vocabulary"
        android:name="com.example.projectdva232v1.ui.Vocabulary.VocabularyFragment"
        android:label="@string/vocabulary"
        tools:layout="@layout/fragment_reading_activity">
        <action
            android:id="@+id/action_nav_vocabulary_to_nav_example5"
            app:destination="@id/nav_example5" />
        <argument
            android:name="VocabularyFragmentArgs"
            app:argType="string"
            android:defaultValue="All" />
    </fragment>
    <fragment
        android:id="@+id/nav_writing"
        android:name="com.example.projectdva232v1.ui.Writing.WritingFragment"
        android:label="@string/writing"
        tools:layout="@layout/fragment_reading_activity">
        <action
            android:id="@+id/action_nav_writing_to_nav_example5"
            app:destination="@id/nav_example5" />
        <argument
            android:name="WritingFragmentArgs"
            app:argType="string"
            android:defaultValue="All" />
    </fragment>
    <fragment
        android:id="@+id/nav_speaking"
        android:name="com.example.projectdva232v1.ui.Speaking.SpeakingFragment"
        android:label="@string/speaking"
        tools:layout="@layout/fragment_reading_activity">
        <action
            android:id="@+id/action_nav_speaking_to_nav_example5"
            app:destination="@id/nav_example5" />
        <argument
            android:name="SpeakingFragmentArgs"
            app:argType="string"
            android:defaultValue="All" />
    </fragment>
    <fragment
        android:id="@+id/nav_reading"
        android:name="com.example.projectdva232v1.ui.Reading.ReadingFragment"
        android:label="@string/reading"
        tools:layout="@layout/fragment_reading_activity">
        <action
            android:id="@+id/action_nav_reading_to_nav_example5"
            app:destination="@id/nav_example5" />
        <argument
            android:name="ReadingFragmentArgs"
            app:argType="string"
            android:defaultValue="All" />
    </fragment>

    <fragment
        android:id="@+id/nav_example5"
        android:name="com.example.projectdva232v1.ui.example5.example5Fragment"
        android:label="@string/reading"
        tools:layout="@layout/fragment_example5" />
    <fragment
        android:id="@+id/nav_result"
        android:name="com.example.projectdva232v1.ui.ResultActivity.ResultActivity"
        android:label="@string/result"
        tools:layout="@layout/result_layout" />
    <fragment
        android:id="@+id/nav_loginActivity"
        android:name="com.example.projectdva232v1.ui.LoginActivity.LoginActivity"
        android:label="@string/login"
        tools:layout="@layout/activity_login" >
        <action
            android:id="@+id/action_nav_loginActivity_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/nav_registrationActivity"
        android:name="com.example.projectdva232v1.ui.RegActivity.RegistrationActivity"
        android:label="@string/register"
        tools:layout="@layout/activity_registration" />
</navigation>